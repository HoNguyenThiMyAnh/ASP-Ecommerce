@model IEnumerable<MyClass.Models.Menu>
@{
    ViewBag.Title = "MenuFooter";
    Layout = "~/Views/Shared/_LayoutSite.cshtml";
}
<h4>Về chúng tôi</h4>
<ul class="list-menu">
    @foreach(var menu in Model)
            {
                <li><a href=" ~/@menu.Link">@menu.Name</a></li>
            }


</ul>


